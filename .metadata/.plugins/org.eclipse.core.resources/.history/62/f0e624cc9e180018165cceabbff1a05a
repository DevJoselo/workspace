package com.projeto.metodos;

public class Metodos {

	public static void main(String[] args) {
		
		int resultado;
		resultado = multiplicaRetorno(3,5);
		System.out.println(resultado + 2);
		
		miMetodo();
		multiplica(2,1);
		
	}
	
	public static void miMetodo(){
		System.out.println("este es mi metodo");
	}

	public static int multiplicaRetorno(int n1, int n2){
		int resultado = 0;
		resultado = n1 * n2;
		return resultado;
	}
	
	public static void multiplica(int numero1, int numero2){
		int resultado = 0;
		resultado = numero1 * numero2;
		System.out.println(resultado);
	}
}
