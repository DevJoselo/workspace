package com.projeto.condicionais;

public class EstruturasCondicionais {

	public static void main(String[] args) {
		// Estruturas Condicionais - if else
		
		int a = 3;
		int b = 2;
		
		if (a == b) {
			System.out.println("son iguales");
		} else {
			System.out.println("son diferentes");
		}
		
	}

}
