package com.projeto.herencia;

public class Animal {

	//Atributos
	private String tamanho;
	private String cor;
	private double peso;
	
	//metodos - acciones
	public void correr(){
		System.out.println("correr como animal");
	}
	
	public void dormir(){
		System.out.println("dormir como animal");
	}
}
